{
    "name": "clarity/currency-list",
    "description": "List of all currencies with names and ISO 4217 codes in all languages and data formats for Laravel",
    "keywords": ["laravel", "currencies"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavels Minckovskis",
            "email": "pavels.minckovskis@clarity.cx",
            "homepage": "https://clarity.cx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.8|^6|^7|^8|^9",
        "umpirsky/currency-list": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Clarity\\CurrencyList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clarity\\CurrencyList\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Clarity\\CurrencyList\\CurrencyListServiceProvider"
            ],
            "aliases": {
                "Currencies": "Clarity\\CurrencyList\\CurrencyListFacade"
            }
        }
    }
}
